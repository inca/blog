{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "description": "Boris Okunskiy Blog",
  "scripts": {
    "clean": "rm -rf out",
    "dev": "run-p dev:*",
    "dev:server": "tsc -p ./dev && run-p dev:server:*",
    "dev:server:tscw": "tsc -w -p ./dev",
    "dev:server:nodemon": "nodemon --delay 0.5 out/dev/index.js",
    "dev:css": "postcss stylesheets/index.css -w -o static/build/index.css",
    "dev:js": "webpack -w",
    "build:js": "NODE_ENV=production webpack",
    "serve": "firebase serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inca/blog.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "Boris Okunskiy",
  "license": "ISC",
  "dependencies": {
    "rho": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/glob": "^7.1.1",
    "@types/json5": "0.0.30",
    "@types/koa": "^2.11.0",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^13.1.8",
    "@types/pug": "^2.0.4",
    "@types/ws": "^7.2.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "debounce": "^1.2.0",
    "glob": "^7.1.6",
    "json5": "^2.1.1",
    "koa": "^2.11.0",
    "koa-router2": "^1.3.1",
    "koa-static": "^5.0.0",
    "moment": "^2.24.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "pug": "^2.0.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10",
    "ws": "^7.2.1"
  }
